group IAccountDaoSql;

insertAccount() ::= <<
    insert into accounts (
      id
      , key_name
    ) values (
      :id
      , :key_name
    );
>> 

getAccountByKey(key) ::= <<
    select
      id
      , key_name
    from accounts
    where
      key_name = :key_name
    ;
>>

getAccountFromId(id) ::= <<
    select
      id
      , key_name
    from accounts
    where
      id = :id
    ;
>>


getAccounts() ::= <<
    select
      id
      , key_name
    from accounts
    ;
>>
